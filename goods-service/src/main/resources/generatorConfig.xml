<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <!--加载属性文件 -->
    <context id="context" targetRuntime="MyBatis3">
        <!-- 实现自定义的代码生成器plugin -->
        <plugin type="com.clubfactory.core.mybatis.PaginationPlugin" />
        <commentGenerator>
            <property name="suppressDate" value="true" />
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="false" />
        </commentGenerator>
        <!-- 数据库连接URL，用户名，密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://rr-bp1bstq8mtcz7qjyzo.mysql.rds.aliyuncs.com:3306/idoo?useUnicode=true&amp;characterEncoding=UTF-8&amp;useSSL=true"
                        userId="reader" password="yuanshujureader" />
        <!--生成模型的包名和位置 -->
        <javaModelGenerator targetPackage="common.model" targetProject="goods-service/src/main/java/" >
            <!-- model 基类 -->
            <property name="rootClass" value="com.clubfactory.core.base.BaseEntity"/>
        </javaModelGenerator>
        <!--映射文件的包名和位置 -->
        <sqlMapGenerator targetPackage="common.model" targetProject="goods-service/src/main/java/" />
        <!--DAO的包名和位置 -->
        <javaClientGenerator targetPackage="common.dao" targetProject="goods-service/src/main/java" type="XMLMAPPER"/>
        <!--要生成哪些表 -->
        <table tableName="purchase_order" enableSelectByExample="false" enableDeleteByExample="false"
               enableCountByExample="false" enableUpdateByExample="false"
               selectByExampleQueryId="false">
        </table>
    </context>
</generatorConfiguration>